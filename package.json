{
  "name": "apollo-link-performance",
  "version": "0.1.1",
  "description": "Easily log and report on the performance of your GraphQL queries.",
  "repository": {
    "type": "git",
    "url": "pregraph/apollo-link-performance"
  },
  "author": "Ryan Hefner <ryan@pregraph.com> (https://www.pregraph.com)",
  "license": "MIT",
  "keywords": [],
  "files": [
    "es",
    "src",
    "types",
    "umd",
    "index.js"
  ],
  "directories": {
    "lib": "/src"
  },
  "main": "index.js",
  "module": "es/index.js",
  "jsnext:main": "src/index.js",
  "types": "types",
  "scripts": {
    "clean": "rimraf index.js es types umd",
    "prebuild": "yarn clean",
    "build": "node ./tools/build.js && yarn types",
    "watch": "babel ./src -d . --ignore __mocks__,__tests__,**/*.test.js --watch",
    "prepare": "yarn build",
    "prepublishOnly": "yarn build",
    "push-release": "git push origin master && git push --tags",
    "dtslint": "dtslint types",
    "test": "snyk test && jest",
    "test:ci": "jest",
    "types": "tsc"
  },
  "peerDependencies": {
    "@apollo/client": ">=3"
  },
  "dependencies": {
    "ololog": "^1.1.175",
    "pretty-bytes": "^5.5.0",
    "quiktime": "^0.1.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.6.9",
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/react": "^18.0.14",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^28.0.3",
    "babel-plugin-dev-expression": "^0.2.2",
    "babel-polyfill": "^6.26.0",
    "dtslint": "^4.0.6",
    "graphql": "^16.5.0",
    "gzip-size": "6.0.0",
    "jest": "^28.0.3",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "snyk": "^1.437.3",
    "typescript": "^4.2.3"
  }
}
